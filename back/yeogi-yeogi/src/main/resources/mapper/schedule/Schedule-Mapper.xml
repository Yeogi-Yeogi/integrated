<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="ScheduleMapper">

    <select id="getScheduleByBoardNo" resultType="com.yeogi.app.notice.dto.NoticeListDto">
        SELECT NO SCHEDULE_NO
                , BOARD_NO
                , TITLE
                , START_TIME
                , LOCATION
        FROM SCHEDULE
        WHERE BOARD_NO = #{boardNo}
    </select>

</mapper>